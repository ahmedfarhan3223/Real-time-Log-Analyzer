{
    "default_log_type": "syslog",
    "default_severity": "INFO",
    "refresh_rate": 100,
    "max_display_logs": 1000,
    "color_scheme": "default",
    "custom_patterns": {
        "myapp": "(?P<timestamp>\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}) \\[(?P<level>\\w+)\\] (?P<service>\\w+) - (?P<message>.*)"
    }
}
